{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "semi": false,
  "tabWidth": 2,
  "endOfLine": "lf",
  "printWidth": 80,
  "bracketSameLine": false,
  "singleAttributePerLine": false,
  "bracketSpacing": true,
  "quoteProps": "as-needed"
}